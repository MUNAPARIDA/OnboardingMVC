@model OnBoardingGIC.Models.Document
@{
    ViewBag.Title = "Upload";
}

<h2>Documents</h2>

@using (Html.BeginForm("Upload", "Document", FormMethod.Post, new { @class = "form-horizontal", role = "form", enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <h4>Upload Your Documents</h4>
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.HiddenFor(model => model.documentId)
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.HiddenFor(model => model.UserID)
    <div class="form-group">
        @Html.LabelFor(m => m.tenStandard, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            <input type="file" name="tenStandard" id="file1" accept=".pdf" /> 
        </div>

    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.twelveStandard, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            <input type="file" name="twelveStandard" id="fileUpload" accept=".pdf" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-success" value="Finish" />
        </div>
    </div>

}









